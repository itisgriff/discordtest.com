{
  "version": 1,
  "include": [
    "/*"
  ],
  "exclude": [
    "/*.ico",
    "/*.png",
    "/*.svg",
    "/*.json",
    "/assets/*"
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/api/[route]",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/vanity/.*",
      "dest": "/index.html"
    },
    {
      "src": "/lookup/.*",
      "dest": "/index.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
} 