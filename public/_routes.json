{
  "version": 1,
  "include": ["/"],
  "exclude": ["/api/*"],
  "routes": [
    {
      "src": "^/vanity/.*",
      "dest": "/index.html"
    },
    {
      "src": "^/lookup/.*",
      "dest": "/index.html"
    },
    {
      "src": "^/vanity$",
      "dest": "/index.html"
    },
    {
      "src": "^/lookup$",
      "dest": "/index.html"
    }
  ]
} 